start:
	a=$webpage -> SystemOpen[a]
	~new ~nb name=($id|$quotedString)
		CreateNotebook[name]
	open ~nb name=($id|$quotedString)
		OpenNotebook[name]
	name=$notebook -> OpenNotebook[name]
	$directory ~dir -> SystemOpen[dir]
notebook:
	spacex -> "SpaceX.nb"
	lui -> "Lui.nb"
directory:
	lui -> $LuiDir
webpage:
	(spacex|spx) reddit -> "https://www.reddit.com/r/spacex/"
~new:
	(create|new)
~nb:
	(notebook|nb)
~dir:
	(directory|dir)
~open:
	(open|go|display)